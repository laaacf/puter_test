{
    "config_name": "Custom Access Config",
    "env": "dev",
    "http_port": 4100,
    "domain": "puter.localhost",
    "protocol": "http",
    "contact_email": "hey@example.com",

    "allow_all_host_values": true,
    "allow_nipio_domains": true,
    "disable_ip_validate_event": true,
    "custom_domains_enabled": true,

    "services": {
        "database": {
            "engine": "sqlite",
            "path": "puter-database.sqlite"
        },
        "dynamo": {
            "path": "./puter-ddb"
        },
        "thumbnails": {
            "engine": "purejs"
        },
        "file-cache": {
            "disk_limit": 16384,
            "disk_max_size": 16384,
            "precache_size": 16384,
            "path": "./file-cache"
        }
    }
}
